{
  "name": "codebasebackend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "gen": "ts-node src/generator/generate-typings.ts",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "webpack": "rimraf dist && webpack --config webpack.config.js --watch",
    "start:hmr": "node dist/main.js",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/federation": "^0.14.1",
    "@apollo/gateway": "^0.14.1",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/graphql": "^7.2.4",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/schedule": "^0.3.0",
    "@nestjs/typeorm": "^7.0.0",
    "apollo-server": "^2.12.0",
    "apollo-server-cache-memcached": "^0.6.4",
    "apollo-server-express": "^2.12.0",
    "apollo-server-plugin-response-cache": "^0.4.1",
    "bcrypt": "^4.0.1",
    "chalk": "^4.0.0",
    "class-transformer": "^0.2.3",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.1.1",
    "graphql": "^15.0.0",
    "graphql-subscriptions": "^1.1.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "node": "^13.12.0",
    "nodemon": "^2.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "typeorm": "^0.2.24",
    "uuid": "^7.0.3",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpackbar": "^4.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/supertest": "^2.0.8",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-loader-helper": "^1.0.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
